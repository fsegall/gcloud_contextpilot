{
  "name": "contextpilot",
  "displayName": "ContextPilot by Livre Solutions",
  "description": "AI-powered context management with multi-agent system and blockchain rewards. By Livre Solutions.",
  "version": "0.4.5",
  "publisher": "livresoltech",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsegall/gcloud_contextpilot"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "ai",
    "context",
    "productivity",
    "agents",
    "blockchain",
    "web3"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:contextpilot.configureRepos"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "contextpilot.connect",
        "title": "ContextPilot: Connect to Backend",
        "icon": "$(plug)"
      },
      {
        "command": "contextpilot.configureRepos",
        "title": "ContextPilot: Configure Repositories",
        "icon": "$(repo)"
      },
      {
        "command": "contextpilot.disconnect",
        "title": "ContextPilot: Disconnect",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "contextpilot.dashboard",
        "title": "ContextPilot: Dashboard",
        "icon": "$(dashboard)"
      },
      {
        "command": "contextpilot.approveProposal",
        "title": "ContextPilot: Approve Proposal",
        "icon": "$(check)"
      },
      {
        "command": "contextpilot.rejectProposal",
        "title": "ContextPilot: Reject Proposal",
        "icon": "$(close)"
      },
      {
        "command": "contextpilot.viewRewards",
        "title": "ContextPilot: View CPT Balance",
        "icon": "$(star)"
      },
      {
        "command": "contextpilot.viewRelatedFiles",
        "title": "ContextPilot: View Related Files",
        "icon": "$(file-text)"
      },
      {
        "command": "contextpilot.askCoach",
        "title": "ContextPilot: Ask Coach",
        "icon": "$(comment)"
      },
      {
        "command": "contextpilot.commitContext",
        "title": "ContextPilot: Commit Context",
        "icon": "$(git-commit)"
      },
      {
        "command": "contextpilot.refreshStatus",
        "title": "ContextPilot: Refresh Status",
        "icon": "$(refresh)"
      },
      {
        "command": "contextpilot.showBackendConfig",
        "title": "ContextPilot: Show Backend Configuration",
        "icon": "$(settings-gear)"
      },
      {
        "command": "contextpilot.getContext",
        "title": "ContextPilot: Get Project Context",
        "icon": "$(folder-library)"
      },
      {
        "command": "contextpilot.commitContextReal",
        "title": "ContextPilot: Commit Context (Real API)",
        "icon": "$(git-commit)"
      },
      {
        "command": "contextpilot.getCoachTipReal",
        "title": "ContextPilot: Get Coach Tip (Real API)",
        "icon": "$(lightbulb)"
      },
      {
        "command": "contextpilot.viewProposalDiff",
        "title": "ContextPilot: View Proposal Diff",
        "icon": "$(diff)"
      },
      {
        "command": "contextpilot.viewProposalChange",
        "title": "ContextPilot: View Proposal File Diff",
        "icon": "$(diff)"
      },
      {
        "command": "contextpilot.askClaudeReview",
        "title": "ContextPilot: Ask Claude to Review",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "contextpilot.resetChatSession",
        "title": "ContextPilot: Reset Chat Session",
        "icon": "$(clear-all)"
      },
      {
        "command": "contextpilot.resetAgentMetrics",
        "title": "ContextPilot: Reset Agent Metrics",
        "icon": "$(refresh)"
      },
      {
        "command": "contextpilot.resetAllAgentMetrics",
        "title": "ContextPilot: Reset All Agents Metrics",
        "icon": "$(sync)"
      },
      {
        "command": "contextpilot.triggerRetrospective",
        "title": "ContextPilot: Start Agent Retrospective",
        "icon": "$(organization)"
      },
      {
        "command": "contextpilot.openContextFile",
        "title": "Open Context File",
        "icon": "$(edit)"
      },
      {
        "command": "contextpilot.initializeContext",
        "title": "ContextPilot: Initialize Project Context",
        "icon": "$(folder-opened)",
        "category": "ContextPilot"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "contextpilot",
          "title": "ContextPilot",
          "icon": "$(rocket)"
        }
      ]
    },
    "views": {
      "contextpilot": [
        {
          "id": "contextpilot.context",
          "name": "ðŸ“¦ Project Context"
        },
        {
          "id": "contextpilot.proposals",
          "name": "Change Proposals"
        },
        {
          "id": "contextpilot.rewards",
          "name": "Rewards"
        },
        {
          "id": "contextpilot.agents",
          "name": "Agents Status"
        },
        {
          "id": "contextpilot.coach",
          "name": "Coach"
        }
      ]
    },
    "configuration": {
      "title": "ContextPilot",
      "properties": {
        "contextpilot.apiUrl": {
          "type": "string",
          "default": "https://contextpilot-backend-581368740395.us-central1.run.app",
          "description": "ContextPilot API URL"
        },
        "contextpilot.mainRepo": {
          "type": "string",
          "default": "",
          "description": "Main GitHub repository (e.g. owner/repo)"
        },
        "contextpilot.sandboxRepoUrl": {
          "type": "string",
          "default": "",
          "description": "Sandbox repository URL (e.g. https://github.com/owner/sandbox.git)"
        },
        "contextpilot.autoConnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically connect on startup"
        },
        "contextpilot.showNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show notifications for agent activities"
        },
        "contextpilot.walletAddress": {
          "type": "string",
          "default": "",
          "description": "Your wallet address for CPT rewards"
        },
        "contextpilot.userId": {
          "type": "string",
          "default": "",
          "description": "Your ContextPilot user ID"
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "contextpilot.connect",
          "when": "!contextpilot.connected"
        },
        {
          "command": "contextpilot.configureRepos"
        },
        {
          "command": "contextpilot.disconnect",
          "when": "contextpilot.connected"
        },
        {
          "command": "contextpilot.askClaudeReview"
        }
      ],
      "view/title": [
        {
          "command": "contextpilot.refreshStatus",
          "when": "view == contextpilot.proposals",
          "group": "navigation"
        },
        {
          "command": "contextpilot.configureRepos",
          "when": "view == contextpilot.proposals || view == contextpilot.context || view == contextpilot.agents",
          "group": "navigation@2"
        }
      ],
      "view/item/context": [
        {
          "command": "contextpilot.openContextFile",
          "when": "view == contextpilot.context && viewItem == project",
          "group": "inline"
        },
        {
          "command": "contextpilot.openContextFile",
          "when": "view == contextpilot.context && viewItem == goal",
          "group": "inline"
        },
        {
          "command": "contextpilot.openContextFile",
          "when": "view == contextpilot.context && viewItem == status",
          "group": "inline"
        },
        {
          "command": "contextpilot.openContextFile",
          "when": "view == contextpilot.context && viewItem == milestones-root",
          "group": "inline"
        },
        {
          "command": "contextpilot.viewProposalDiff",
          "when": "view == contextpilot.proposals && viewItem == proposal",
          "group": "navigation@1"
        },
        {
          "command": "contextpilot.viewProposalChange",
          "when": "view == contextpilot.proposals && viewItem == proposalChange",
          "group": "inline"
        },
        {
          "command": "contextpilot.askClaudeReview",
          "when": "view == contextpilot.proposals && viewItem == proposal",
          "group": "inline@1"
        },
        {
          "command": "contextpilot.viewRelatedFiles",
          "when": "view == contextpilot.proposals && viewItem == proposal",
          "group": "navigation@2"
        },
        {
          "command": "contextpilot.approveProposal",
          "when": "view == contextpilot.proposals && viewItem == proposal",
          "group": "inline"
        },
        {
          "command": "contextpilot.rejectProposal",
          "when": "view == contextpilot.proposals && viewItem == proposal",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack",
    "dev": "cursor --extensionDevelopmentPath=/home/fsegall/Desktop/New_Projects/google-context-pilot/extension",
    "compile": "tsc -p ./",
    "webpack": "webpack --mode production",
    "webpack:watch": "webpack --mode development --watch",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.x",
    "ts-loader": "^9.5.4",
    "typescript": "^5.3.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ethers": "^6.9.0",
    "simple-git": "^3.28.0"
  }
}
